{
	"name": "tree-generator",
	"displayName": "tree-generator",
	"description": "这是一个可以方便快速生成文件目录树的插件",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.10.0"
	},
	"publisher": "xboxyan",
	"icon": "icon.png",
	"keywords": [
		"tree",
		"tree-generator",
		"树"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:extension.generator"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.generator",
            "title": "生成目录树"

		}],
		"menus": {
            "explorer/context": [
                {
					"when": "explorerResourceIsFolder",
                    "command": "extension.generator",
                    "group": "navigation"
                }
            ]
        }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.10.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
		"mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }

}
